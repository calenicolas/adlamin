{
  "containerConfiguration": {
    "containerName": "deploy_service",
    "imageName": "deploy-service",
    "networks": [{
      "name": "internal"
    }],
    "servicePorts": ["8080"],
    "mounts": [{
      "type": "bind",
      "source": "/root/deploy-service/deploy/pending",
      "target": "/etc/pending-deploys"
    }]
  },
  "portKnockingConfiguration": {
    "sequence": "4004 5005 6006",
    "serviceProtocol": "tcp",
    "publicPort": "2002",
    "publicNetwork": "eth0"
  }
}
