{
  "containerConfiguration": {
    "imageName": "nginx:latest",
    "networks": [{
      "name": "internal"
    }],
    "servicePorts": ["80"],
    "mounts": [{
      "type": "bind",
      "source": "/etc/adlamin/reverse_proxy/dmz/nginx.conf",
      "target": "/etc/nginx/conf.d/default.conf"
    }],
    "extraOptions": "--memory=150m --cpus=\".3\""
  },
  "reverseProxyConfiguration": {
    "publicPort": 80,
    "publicNetwork": "eth0"
  }
}