{
  "containerConfiguration": {
    "containerName": "dmz",
    "imageName": "nginx:latest",
    "networks": [{
      "name": "internal"
    }],
    "servicePorts": ["80"],
    "mounts": [{
      "type": "bind",
      "source": "/etc/adlamin/configuration/nginx",
      "target": "/etc/nginx/conf.d"
    }],
    "extraOptions": "--memory=150m --cpus=\".3\""
  },
  "reverseProxyConfiguration": {
    "publicPort": 80,
    "publicNetwork": "eth0"
  }
}