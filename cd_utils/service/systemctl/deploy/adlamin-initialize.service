[Unit]
Description=Initializer for adlamin deploy service
After=docker.service adlamin-cleanup.service

[Service]
Type=oneshot
ExecStart=/usr/local/bin/adlamin-cli initialize
ExecReload=/usr/local/bin/adlamin-cli initialize
StandardOutput=journal+console
StandardError=journal+console

[Install]
WantedBy=default.target